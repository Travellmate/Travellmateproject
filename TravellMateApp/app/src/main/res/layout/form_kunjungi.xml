<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="id.admintravellmate.travellmateapp.MainActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@drawable/booking_sekarangbangkok" />
    <LinearLayout
        android:id="@+id/layout1"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_below="@+id/layout7"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:background="#d0fdfcfb"
        android:gravity="top"
        android:orientation="horizontal">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout2"
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_below="@+id/layout1"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:background="#d0fdfcfb"
        android:gravity="top"
        android:orientation="horizontal">
        <Button
            android:id="@+id/buttonX"
            android:layout_width="10dp"
            android:layout_height="23dp"
            android:layout_marginLeft="320dp"
            android:layout_marginRight="15dp"
            android:layout_weight="1"
            android:background="#0d0d0d"
            android:text="X"
            android:textColor="@android:color/white"
            android:textSize="18sp"
            android:textStyle="normal|bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout3"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:layout_below="@+id/layout2"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:background="#d0fdfcfb"
        android:gravity="top"
        android:orientation="horizontal">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout4"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_below="@+id/layout2"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="350dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:gravity="left|center"
            android:text="Nama Anda"
            android:textColor="#080808"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout5"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@+id/layout4"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView1"
            android:labelFor="@+id/autoCompleteTextView1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="#fdfdfd" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout6"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_below="@+id/layout5"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView3"
            android:layout_width="350dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:gravity="left|center"
            android:text="Email"
            android:textColor="#080808"
            android:textSize="15sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout8"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@+id/layout6"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView2"
            android:labelFor="@+id/autoCompleteTextView2"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="#fdfdfd" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout9"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_below="@+id/layout8"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView4"
            android:layout_width="350dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:gravity="left|center"
            android:text="No Hp/WA/Line"
            android:textColor="#080808"
            android:textSize="15sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout10"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@+id/layout9"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView3"
            android:labelFor="@+id/autoCompleteTextView3"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="#fdfdfd" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout11"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_below="@+id/layout10"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView5"
            android:layout_width="350dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:gravity="left|center"
            android:text="Destinasi Tour"
            android:textColor="#080808"
            android:textSize="15sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout12"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@+id/layout11"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">

        <ExpandableListView
            android:id="@+id/expandableListView1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="#fdfdfd" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout13"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_below="@+id/layout12"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView6"
            android:layout_width="350dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:gravity="left|center"
            android:text="Info Lengkap"
            android:textColor="#080808"
            android:textSize="15sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout14"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_below="@+id/layout13"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView4"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="#fdfdfd"
            android:labelFor="@+id/autoCompleteTextView4" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout15"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_below="@+id/layout14"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#d6e0f2"
        android:gravity="top"
        android:orientation="horizontal">
    <Button
        android:id="@+id/buttonKirim"
        android:layout_width="182dp"
        android:layout_height="35dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="220dp"
        android:layout_weight="1"
        android:background="#476bc8"
        android:text="Kirim"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold" />
    </LinearLayout>
</RelativeLayout>