<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="id.admintravellmate.travellmateapp.MainActivity">

    <LinearLayout
        android:id="@+id/layout1"
        android:background="#d70d0d0d"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="center">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="10dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:gravity="center"
            app:srcCompat="@drawable/panah" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="100dp"
            android:layout_height="60dp"
            android:gravity="center"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/textView1"
            android:textSize="35sp"
            android:textColor="#4d94eb"
            android:layout_width="210dp"
            android:layout_height="90dp"
            android:gravity="center"
            android:textStyle="normal|bold"
            android:text="TravellMate" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout2"
        android:layout_below="@+id/layout1"
        android:background="#71a2ec"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:gravity="left|center"
            android:text="Seoul"
            android:textColor="#fbfbfc"
            android:textSize="20sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout3"
        android:layout_below="@+id/layout2"
        android:background="#71a2ec"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:gravity="left|center"
            android:text="Explore South Korea"
            android:textColor="#fbfbfc"
            android:textSize="15sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout4"
        android:layout_below="@+id/layout3"
        android:background="#fcfdfd"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center">
        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:gravity="center"
            app:srcCompat="@drawable/korea" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout5"
        android:layout_below="@+id/layout4"
        android:background="#fcfdfd"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center">
        <Button
            android:id="@+id/buttonKunjungi1"
            android:layout_width="182dp"
            android:layout_height="25dp"
            android:layout_marginLeft="110dp"
            android:layout_marginRight="110dp"
            android:layout_weight="1"
            android:background="#476bc8"
            android:text="Kunjungi Sekarang!"
            android:textColor="@android:color/white"
            android:textSize="9sp"
            android:textStyle="normal|bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout6"
        android:layout_below="@+id/layout5"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="15dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:layout_marginRight="50dp"
            android:layout_marginLeft="20dp"
            android:text="Wisata Sejarah"
            android:gravity="center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>
        <TextView
            android:id="@+id/textView11"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="50dp"
            android:text="Wisata Belanja"
            android:gravity="right|center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout7"
        android:layout_below="@+id/layout6"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:gravity="center">
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="50dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:gravity="center"
            app:srcCompat="@drawable/gyeongbukgungpalace" />
        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="50dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:gravity="center"
            app:srcCompat="@drawable/myeongdong" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout8"
        android:layout_below="@+id/layout7"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="17dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="180dp"
            android:layout_height="90dp"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="15dp"
            android:text="Gyeongbukgung Palace"
            android:gravity="center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>
        <TextView
            android:id="@+id/textView12"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="50dp"
            android:text="Myeongdong"
            android:gravity="center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout9"
        android:layout_below="@+id/layout8"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="160dp"
            android:layout_height="60dp"
            android:layout_marginLeft="10dp"
            android:gravity="left"
            android:text="
- Alamat : 161 Sajik-ro, Sejongno,
Jongno-gu, Seoul, South Korea
- Tiket Masuk : 3.000 won
- Jam Operasional : 09.00 - 17.00"
            android:textColor="#0e0e0e"
            android:textSize="9sp" />
        <TextView
            android:id="@+id/textView13"
            android:layout_width="180dp"
            android:layout_height="60dp"
            android:layout_marginRight="2dp"
            android:layout_marginLeft="20dp"
            android:gravity="left"
            android:text="
- Alamat : Myeongdong-gil, Jung-gu, Seoul
- Tiket Masuk : Gratis
- Jam Operasional : 07.00- 23.00"
            android:textColor="#0e0e0e"
            android:textSize="9sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout10"
        android:layout_below="@+id/layout9"
        android:background="#d7fafafa"
        android:orientation="horizontal"
        android:layout_width="180dp"
        android:layout_height="5dp"
        android:gravity="center">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout11"
        android:layout_below="@+id/layout10"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="15dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_marginRight="50dp"
            android:layout_marginLeft="30dp"
            android:text="Wisata Kuliner"
            android:gravity="center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>
        <TextView
            android:id="@+id/textView15"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="50dp"
            android:text="Wisata Religi"
            android:gravity="right|center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout12"
        android:layout_below="@+id/layout11"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:gravity="center">
        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="50dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:gravity="center"
            app:srcCompat="@drawable/gwangjang" />
        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="50dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:gravity="center"
            app:srcCompat="@drawable/bongeunsa" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout13"
        android:layout_below="@+id/layout12"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="17dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView17"
            android:layout_width="180dp"
            android:layout_height="90dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="70dp"
            android:text="Gwangjang Market"
            android:gravity="center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>
        <TextView
            android:id="@+id/textView18"
            android:layout_width="180dp"
            android:layout_height="90dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="60dp"
            android:text="Bongeunsa Temple"
            android:gravity="center"
            android:textColor="#0e0e0e"
            android:textSize="15sp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout14"
        android:layout_below="@+id/layout13"
        android:background="#d7e2e1e1"
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:gravity="center">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="160dp"
            android:layout_height="60dp"
            android:layout_marginLeft="5dp"
            android:gravity="left"
            android:text="
- Alamat : 88, Changgyeonggung -ro,
Jongno-gu, Seoul 03195, Korea Selatan (Gwanghwamun/ Jongno)
- Tiket Masuk : Gratis
- Jam Operasional : 06.00 - 22.00"
            android:textColor="#0e0e0e"
            android:textSize="9sp" />
        <TextView
            android:id="@+id/textView19"
            android:layout_width="180dp"
            android:layout_height="60dp"
            android:layout_marginRight="2dp"
            android:layout_marginLeft="20dp"
            android:gravity="left"
            android:text="
- Alamat : 531 Bongeunsa-ro, Samseong 1
(il)-dong, Gangnam-gu, Seoul, South Korea
- Tiket Masuk : 3.000 won
- Jam Operasional : 09.00 - 17.00"
            android:textColor="#0e0e0e"
            android:textSize="9sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout15"
        android:layout_below="@+id/layout14"
        android:background="#d7fafafa"
        android:orientation="horizontal"
        android:layout_width="180dp"
        android:layout_height="5dp"
        android:gravity="center">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout16"
        android:background="#d70d0d0d"
        android:layout_below="@+id/layout15"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="center">
        <TextView
            android:id="@+id/textView10"
            android:textSize="15sp"
            android:textColor="#f9fafc"
            android:layout_width="300dp"
            android:layout_height="90dp"
            android:gravity="center"
            android:text="Travell Mate © 2017 All Rights Reserved." />
    </LinearLayout>

</RelativeLayout>